цел кол_во    = 1000;
цел делитель1 = 3;
цел делитель2 = 5;
цел сумма     = 0;
сумма = сумма + делитель1 * ((кол_во - 1) / делитель1) * ((кол_во - 1) / делитель1 + 1) / 2;
сумма = сумма + делитель2 * ((кол_во - 1) / делитель2) * ((кол_во - 1) / делитель2 + 1) / 2;
цел пересечение = делитель1 * делитель2;
сумма = сумма - пересечение * ((кол_во - 1) / пересечение) * ((кол_во - 1) / пересечение + 1) / 2;
<< сумма;  /* Ответ: 233168 */
