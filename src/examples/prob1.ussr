цел32 кол_во    = 1000;
цел32 делитель1 = 3;
цел32 делитель2 = 5;
цел32 сумма     = 0;
сумма += делитель1 * ((кол_во - 1) / делитель1) * ((кол_во - 1) / делитель1 + 1) / 2;
сумма += делитель2 * ((кол_во - 1) / делитель2) * ((кол_во - 1) / делитель2 + 1) / 2;
пересечение = делитель1 * делитель2;
сумма -= пересечение * ((кол_во - 1) / пересечение) * ((кол_во - 1) / пересечение + 1) / 2;
вывод сумма;
